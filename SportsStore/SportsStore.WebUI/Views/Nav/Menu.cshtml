@*@model IEnumerable<string>*@
@model IOrderedEnumerable<Tuple <string, string>>
@*<li>
    @Html.ActionLink("Главная", "List", "Product")
</li>*@
@foreach (var link in Model)
{
    <li>
        @Html.RouteLink(link.Item2, new
{
    controller = "Product",
    action = "List",
    category = link.Item1,
   // page = 1
},
new
{
    @class = link.Item1 == ViewBag.SelectedCategory ? "selected" : null
})
    </li>
}
